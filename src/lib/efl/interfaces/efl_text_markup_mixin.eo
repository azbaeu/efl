import efl_text_types;

mixin Efl.Text.Markup_Mixin {
   [[Markup mixin

     @since 1.22
   ]]
   data: Efl_Text_Markup_Mixin_Data;
   methods {
      markup_insert {
         [[Inserts markup at position of cursor]]
         params {
            cur: ptr(Efl.Text.Cursor.Cursor);
            markup: string;
         }
      }
   }
}

